{
  "manifest_version": 3,
  "name": "Matrics event manager",
  "version": "1.0.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "./src/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.meetup.com/*/schedule/"
      ],
      "js": [
        "./src/main.ts"
      ]
    }
  ]
}